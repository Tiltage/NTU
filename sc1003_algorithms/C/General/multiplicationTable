#include <stdio.h>
int main() 
{
	/*edit*/
   /* Write your code here */
    int row,i,num;
    printf("Enter a number (between 1 and 9): \n");
    scanf("%d",&num);
    printf("Multiplication Table: \n");
    printf(" ");
    for (i=1;i<num+1;i++)
    {
        printf("%d ",i);
    }
    printf("\n");
    for (row=1;row<num+1;row++)
    {
        printf("%d ",row);
        for (i=1;i<row+1;i++)
        {
            printf("%d ",i*row);
        }
        printf("\n");
            
    }
   return 0;  
}
