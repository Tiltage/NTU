#include <stdio.h>
int main() 
{
	/*edit*/
   /* Write your code here */
   char q;
   printf("Enter a character: \n");
   scanf("%c",&q);
   //printf("%d",q);
   if (q>47 && q<58)
   {
       printf("Digit");
   }
   else if (q>64 && q<91)
   {
       printf("Upper case letter");
   }
   else if (q>96 && q<122)
   {
       printf("Lower case letter");
   }
   else
   {
       printf("Other character");
   }

   return 0;  
}
