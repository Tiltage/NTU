#include <stdio.h>
int main()
{
	/*edit*/
   /* Write your code here */
   int i,h,k,num;
   printf("Enter the height:  \n");
   scanf("%d",&h);
   printf("The pattern is:  \n");
   for (i=1;i<=h;i++)
   {
       num=i;
       for(k=0;k<i;k++)
       {
           printf("%d",(num++%10));
       }
       printf("\n");
   }


	/*end_edit*/
   return 0;
}
