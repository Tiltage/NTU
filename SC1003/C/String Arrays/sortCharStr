#include <stdio.h>
void sortCharStr(char *str);
int main()
{
   char str[80];
   
   printf("Enter a string: \n");
   scanf("%s",str);
   sortCharStr(str);
   printf("sortCharStr(): %s\n", str);
   return 0;
}
void sortCharStr(char *str)
{
	/*edit*/
/* Write your code here */
int i,j,count=0;
int len = strlen(str);
char temp;
for (i=0;i<len-1;i++)
{
    for (j=0;j<len-1;j++)
    {
        if (*(str+j)>*(str+j+1))
        {
            temp = *(str+j);
            *(str+j) = *(str+j+1);
            *(str+j+1) = temp;
        }
    }

}


	/*end_edit*/
}
