//longestStrInAr

int index,stringlen=0,i,string;
*length=0;
for (index=0;index<size;index++)
{
    stringlen=0;    
    for (i=0;str[index][i]!='\0';i++)
    {
        stringlen++;
        //printf("%d",index);
    }
    if (stringlen>*length)
    {
        *length=stringlen;
        string=index;
    }
    //printf("%d",string);
}
return str[string];
